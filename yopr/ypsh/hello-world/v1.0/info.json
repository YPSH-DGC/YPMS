{
    "release.name": "Version 1.0",
    "release.dependencies": {
        "yopr/@system/ypsh": ">1.0"
    },
    "release.guides": {
        "install": {
            "steps": [
                {
                    "type": "license-agreement-url",
                    "content": "https://raw.githubusercontent.com/YPSH-DGC/YPMS/refs/heads/main/LICENSE"
                },
                {
                    "type": "download-only",
                    "content": {
                        "dest": "{YPMS_ENV_DIR}/hello-world/hello-world.py",
                        "url": "https://ypsh-dgc.github.io/YPMS/yopr/ypsh/hello-world/v1.0/hello-world.py"
                    }
                }
            ]
        },
        "uninstall": {
            "type": "remove-file",
            "content": "{YPMS_ENV_DIR}/hello-world/hello-world.py"
        }
    }
}